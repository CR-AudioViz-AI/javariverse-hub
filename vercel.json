{
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "github": {
    "enabled": true,
    "autoAlias": true,
    "silent": false
  },
  "crons": [
    {
      "path": "/api/bots/conductor",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/bots/sentinel",
      "schedule": "*/10 * * * *"
    },
    {
      "path": "/api/bots/guardian",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/api/bots/architect",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/bots/sentinel-db",
      "schedule": "*/10 * * * *"
    },
    {
      "path": "/api/bots/oracle",
      "schedule": "*/30 * * * *"
    },
    {
      "path": "/api/bots/sentinel-api",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/mortgage-rates",
      "schedule": "0 * * * *"
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}
